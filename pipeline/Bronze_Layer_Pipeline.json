{
	"name": "Bronze_Layer_Pipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy ServiceCenter Table Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "00:30:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ServiceCenterID",
									"type": "int",
									"physicalType": "int",
									"precision": 10
								},
								"sink": {
									"name": "ServiceCenterID"
								}
							},
							{
								"source": {
									"name": "Name",
									"type": "varchar",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Name"
								}
							},
							{
								"source": {
									"name": "Region",
									"type": "varchar",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Region"
								}
							},
							{
								"source": {
									"name": "Address",
									"type": "varchar",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Address"
								}
							},
							{
								"source": {
									"name": "ManagerName",
									"type": "varchar",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "ManagerName"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Bikes_Table_Dataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Bikes_Bronze_Layer_Dataset",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}