{
	"name": "Bikes_Ingestion_Dataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "RideFixr_DataLake_LinkedService",
			"type": "LinkedServiceReference",
			"parameters": {
				"datalake_connection_url": "https://ridefixrdeltalake.dfs.core.windows.net/"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": "ingestion_layer/Bikes",
				"fileSystem": "medallion-layer"
			},
			"compressionCodec": "snappy"
		},
		"schema": [
			{
				"name": "BikeID",
				"type": "INT32"
			},
			{
				"name": "CustomerID",
				"type": "INT32"
			},
			{
				"name": "Make",
				"type": "UTF8"
			},
			{
				"name": "Model",
				"type": "UTF8"
			},
			{
				"name": "Year",
				"type": "INT32"
			},
			{
				"name": "RegistrationNo",
				"type": "UTF8"
			}
		]
	}
}